@using System.Diagnostics.Eventing.Reader
@using PagedList.Mvc
@model PagedList.IPagedList<ICM.Web.Models.ProvinceOrState>

<table class="table">
    <tr>
        <th></th>
        <th>
            @Html.DisplayNameFor(model => model.First().Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().By)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().CreatedOrUpdated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().Version)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().Active)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.ProvinceOrStateID}) |
                @Html.ActionLink("Details", "Details", new {id = item.ProvinceOrStateID})
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.By)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOrUpdated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Version)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>

        </tr>
    }
</table>

