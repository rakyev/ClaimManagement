                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CompanyBranchID)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.WebAccessCode)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Address2)
                        </td>
                        <td>
